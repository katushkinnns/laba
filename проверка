def gcd(a, b):
    while b:
        a, b = b, a % b
    return a
A = int(input("Введите первое положительное число A: "))
B = int(input("Введите второе положительное число B: "))
if A > 0 and B > 0:
    result = gcd(A, B)
    print(f"Наибольший общий делитель {A} и {B} равен {result}.")
else:
    print("Пожалуйста, введите положительные целые числа.")